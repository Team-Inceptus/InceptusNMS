{
    "$id": "inceptusnms.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": true,
    "definitions": {
        "javatype": {
            "type": "string",
            "enum": [
                "class",
                "interface",
                "enum",
                "record",
                "annotation"
            ],
            "description": "The Java type of the class"
        },
        "comment": {
            "type": "string",
            "description": "The JavaDoc comment value of an element"
        },
        "annotations": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "additionalProperties": true,
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "description": "The Java type of the annotation"
                    },
                    "params": {
                        "$ref": "#/definitions/params"
                    }
                }
            }
        },
        "params": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "additionalProperties": true,
                "type": "object",
                "properties": {
                    "type": {
                        "type": "string",
                        "description": "The Java type of the parameter"
                    },
                    "name": {
                        "type": "string",
                        "description": "The case-sensitive name of the parameter"
                    },
                    "comment": {
                        "$ref": "#/definitions/comment"
                    }
                },
                "required": [
                    "type",
                    "name",
                    "comment"
                ]
            },
            "description": "The parameters of a method or constructor"
        },
        "visbility": {
            "type": "string",
            "enum": [
                "public",
                "package",
                "protected",
                "private"
            ],
            "default": "public",
            "description": "The visbility of an element"
        },
        "mods": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "additionalProperties": false,
                "type": "string",
                "enum": [
                    "abstract",
                    "static",
                    "final",
                    "strictfp",
                    "volatile",
                    "transient"
                ]
            },
            "description": "The java modifiers of an element"
        },
        "enumeration": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "name": {
                    "type": "string",
                    "description": "The name of the enum constant"
                },
                "annotations": {
                    "$ref": "#/definitions/annotations"
                },
                "comment": {
                    "$ref": "#/definitions/comment"
                }
            },
            "required": [
                "name",
                "comment"
            ],
            "description": "Enumeration definition"
        },
        "field": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "type": {
                    "type": "string"
                },
                "visbility": {
                    "$ref": "#/definitions/visbility"
                },
                "mods": {
                    "$ref": "#/definitions/mods"
                },
                "annotations": {
                    "$ref": "#/definitions/annotations"
                },
                "comment": {
                    "$ref": "#/definitions/comment"
                }
            },
            "required": [
                "type",
                "comment"
            ],
            "description": "Field definition"
        },
        "constructor": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "visbility": {
                    "$ref": "#/definitions/visbility"
                },
                "params": {
                    "$ref": "#/definitions/params"
                },
                "annotations": {
                    "$ref": "#/definitions/annotations"
                },
                "comment": {
                    "$ref": "#/definitions/comment"
                }
            },
            "required": [
                "comment"
            ],
            "description": "Constructor definition"
        },
        "method": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "visbility": {
                    "$ref": "#/definitions/visbility"
                },
                "mods": {
                    "$ref": "#/definitions/mods"
                },
                "params": {
                    "$ref": "#/definitions/params"
                },
                "return": {
                    "type": "string",
                    "default": "void",
                    "description": "The return type of the method"
                },
                "throws": {
                    "type": "array",
                    "additionalItems": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "The exceptions thrown by the method"
                },
                "annotations": {
                    "$ref": "#/definitions/annotations"
                },
                "comment": {
                    "$ref": "#/definitions/comment"
                }
            },
            "required": [
                "comment"
            ],
            "description": "Method definition"
        }
    },
    "properties": {
        "class": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "type": {
                    "$ref": "#/definitions/javatype"
                },
                "enclosing": {
                    "type": "string",
                    "description": "The enclosing class of this class (if applicable)"
                },
                "visbility": {
                    "$ref": "#/definitions/visbility"
                },
                "mods": {
                    "$ref": "#/definitions/mods"
                },
                "extends": {
                    "type": "string",
                    "description": "The class this class extends (if applicable)"
                },
                "implements": {
                    "type": "array",
                    "additionalItems": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "The interfaces this class implements (if applicable)"
                },
                "annotations": {
                    "$ref": "#/definitions/annotations"
                },
                "comment": {
                    "$ref": "#/definitions/comment"
                }
            },
            "description": "Class definition",
            "required": [
                "type",
                "comment"
            ]
        },
        "enumerations": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "$ref": "#/definitions/enumeration"
            },
            "description": "Enumeration definitions"
        },
        "fields": {
            "type": "object",
            "additionalProperties": true,
            "patternProperties": {
                "[\\s\\S]*": {
                    "$ref": "#/definitions/field"
                }
            },
            "description": "Field definitions"
        },
        "constructors": {
            "type": "array",
            "additionalItems": true,
            "items": {
                "$ref": "#/definitions/constructor"
            },
            "description": "Constructor definitions"
        },
        "methods": {
            "type": "object",
            "additionalProperties": true,
            "patternProperties": {
                "[\\s\\S]*": {
                    "$ref": "#/definitions/method"
                }
            },
            "description": "Method definitions"
        }
    },
    "title": "InceptusNMS Documentation",
    "description": "InceptusNMS Documentation Schema for a class docs JSON file",
    "type": "object"
}